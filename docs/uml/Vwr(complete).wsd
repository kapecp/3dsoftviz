	crGr() -- rstMnpGpr()
	crGr() -- rstVisGrp()
	crGr() -- appCfg()
	
	dtHlp() -- osg()
	dtHlp() -- dtNd()
	dtHlp() -- dtEg()
	dtHlp() -- dtTp()
	
	egGrp() -- dtEg()
	egGrp() -- osg()
	
	msCtrl() -- vwrQt()
	msCtrl() -- crWnd()
	
	ndGrp() -- osg()
